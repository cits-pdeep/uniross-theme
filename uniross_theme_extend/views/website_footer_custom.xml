    <template id="footer_copyright_company_name" inherit_id="website.layout">
        <xpath expr="//footer//span[hasclass('o_footer_copyright_name')]"
               position="replace">
            <span class="o_footer_copyright_name me-2">Copyright @2022 Uniross All rights
                reserved | Terms Of Use | Terms Of Sale | Privacy Policy
            </span>

            <span class="s_social_media o_not_editable des_margin_left_38 text-end"
                  data-snippet="s_social_media" data-name="Social Media">
                <h4 class="s_social_media_title d-none o_default_snippet_text">Social
                    Medias
                </h4>
                <a target="_blank" href="/website/social/facebook"
                   class="s_social_media_facebook" aria-label="Facebook">
                    <i class="fa o_editable_media fa-facebook fa-stack text-o-color-1"/>
                </a>
                <a target="_blank" href="/website/social/youtube"
                   class="s_social_media_youtube" aria-label="YouTube">
                    <i class="fa o_editable_media fa-youtube fa-stack text-o-color-1"/>
                </a>
                <a target="_blank" href="/website/social/instagram"
                   class="s_social_media_instagram" aria-label="Instagram">
                    <i class="fa o_editable_media fa-instagram fa-stack text-o-color-1"/>
                </a>
            </span>
        </xpath>
